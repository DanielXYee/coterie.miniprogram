#member-manage .head {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  background-color: #ffffff;
  display: flex;
  align-items: center;
  justify-content: center;
  padding: 15px;
}
#member-manage .head .search {
  flex: 1;
  position: relative;
  height: 40px;
  line-height: 40px;
  border-radius: 4px;
  background-color: #F3F3F3;
}
#member-manage .head .search input {
  padding-left: 34px;
  height: 40px;
  line-height: 40px;
  font-size: 12px;
}
#member-manage .head .search .input-placeholder {
  color: #909090;
  font-size: 12px;
}
#member-manage .head .search span {
  position: absolute;
  top: 0;
  left: 10px;
  color: #909090;
  font-size: 14px;
}
#member-manage .head .cancel {
  height: 40px;
  line-height: 40px;
  color: #202020;
  font-size: 13px;
  padding: 0 2px 0 17px;
}
#member-manage .content {
  margin-top: 70px;
}
#member-manage .content .manage-item .title {
  height: 26px;
  line-height: 26px;
  padding-left: 15px;
  color: #909090;
  font-size: 11px;
}
#member-manage .content .manage-item .member-list {
  background-color: #ffffff;
}
#member-manage .content .manage-item .member-list .item {
  padding: 0 16px 0 15px;
  height: 50px;
  display: flex;
  align-items: center;
  box-shadow: 0px 0px 0px 0px rgba(0, 0, 0, 0.1);
}
#member-manage .content .manage-item .member-list .item .avatar {
  width: 30px;
  height: 30px;
  margin-right: 10px;
}
#member-manage .content .manage-item .member-list .item .avatar image {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
#member-manage .content .manage-item .member-list .item .nick-name {
  flex: 1;
  font-size: 14px;
  color: #4A4A4A;
}
#member-manage .content .manage-item .member-list .item .iconfont {
  font-size: 12px;
  color: #909090;
}
#member-manage .mask {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 5;
  display: none;
}
#member-manage .mask.cur {
  display: block;
}
#member-manage .permission {
  height: 0;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  text-align: center;
  background-color: #ffffff;
  z-index: 10;
}
#member-manage .permission .item {
  height: 50px;
  line-height: 50px;
  font-size: 16px;
  color: #202020;
}
#member-manage .permission .item.cancel {
  color: #004E9D;
}
#member-manage .permission .item.bor {
  height: 56px;
  line-height: 56px;
  border-bottom: 6px solid #E6E6E6;
}
#member-manage .permission.cur {
  height: 260px;
}
